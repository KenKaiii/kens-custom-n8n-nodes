{
	"name": "@kenkaiii/n8n-nodes-supercode",
	"version": "1.0.71",
	"description": "Advanced JavaScript/Python execution nodes for n8n with 44 JavaScript libraries and 38+ Python packages with AI agent support including YouTube downloading and FFmpeg video processing",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"javascript",
		"python",
		"code-execution",
		"ai-agent",
		"supercode"
	],
	"license": "MIT",
	"author": {
		"name": "Ken Kai",
		"email": "ken@kenkais.com"
	},
	"engines": {
		"node": ">=20.18"
	},
	"main": "index.js",
	"scripts": {
		"build": "rimraf dist && tsc && npm run build:icons && npm run flatten && npm run webpack",
		"webpack": "npx webpack --config webpack.config.js",
		"build:icons": "gulp --gulpfile gulpfile.cjs build:icons",
		"flatten": "cp -r dist/nodes/* dist/ && rm -rf dist/nodes && cp package.json index.js python-installer.py dist/ && cp -r security dist/",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes --ext .ts",
		"lintfix": "eslint nodes --ext .ts --fix",
		"test": "jest --passWithNoTests",
		"test:watch": "jest --watch",
		"prepublishOnly": "npm run build && npm run test"
	},
	"files": [
		"dist/**/*",
		"dist/",
		"index.js",
		"python-installer.py",
		"LICENSE.md"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [],
		"nodes": [
			"dist/SuperCodeNode/SuperCodeNode.node.js",
			"dist/SuperCodeTool/SuperCodeTool.node.js"
		]
	},
	"dependencies": {
		"@distube/ytdl-core": "^4.15.1",
		"ajv": "^8.17.1",
		"archiver": "^7.0.1",
		"axios": "^1.6.0",
		"bcryptjs": "^3.0.2",
		"bufferutil": "^4.0.8",
		"bytes": "^3.1.2",
		"cheerio": "^1.0.0-rc.12",
		"crypto-js": "^4.2.0",
		"csv-parse": "^5.5.2",
		"currency.js": "^2.0.4",
		"date-fns": "^4.1.0",
		"date-fns-tz": "^3.2.0",
		"dayjs": "^1.11.10",
		"ethers": "^6.15.0",
		"fast-xml-parser": "^5.2.5",
		"ffmpeg-static": "^5.2.0",
		"fluent-ffmpeg": "^2.1.2",
		"form-data": "^4.0.4",
		"fuse.js": "^7.1.0",
		"handlebars": "^4.7.8",
		"iban": "^0.0.14",
		"ini": "^5.0.0",
		"jimp": "^1.6.0",
		"joi": "^17.11.0",
		"jsonwebtoken": "^9.0.2",
		"knex": "^3.1.0",
		"libphonenumber-js": "^1.12.10",
		"lodash": "^4.17.21",
		"mathjs": "^14.6.0",
		"moment-timezone": "^0.6.0",
		"ms": "^2.1.3",
		"nanoid": "^5.1.5",
		"node-forge": "^1.3.1",
		"papaparse": "^5.5.3",
		"pdf-lib": "^1.17.1",
		"pluralize": "^8.0.0",
		"qrcode": "^1.5.4",
		"qs": "^6.14.0",
		"slug": "^11.0.0",
		"string-similarity": "^4.0.4",
		"toml": "^3.0.0",
		"utf-8-validate": "^6.0.4",
		"uuid": "^9.0.1",
		"validator": "^13.11.0",
		"web3": "^4.16.0",
		"xlsx": "^0.18.5",
		"xml2js": "^0.6.2",
		"yaml": "^2.8.1",
		"yup": "^1.7.0"
	},
	"devDependencies": {
		"@types/crypto-js": "^4.2.1",
		"@types/fluent-ffmpeg": "^2.1.24",
		"@types/jest": "^29.5.14",
		"@types/lodash": "^4.17.0",
		"@types/uuid": "^9.0.7",
		"@types/validator": "^13.11.7",
		"@typescript-eslint/eslint-plugin": "^8.39.0",
		"@typescript-eslint/parser": "^8.39.0",
		"eslint": "^9.17.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.3",
		"gulp": "^5.0.0",
		"jest": "^29.7.0",
		"prettier": "^3.4.2",
		"rimraf": "^6.0.1",
		"ts-jest": "^29.2.5",
		"typescript": "^5.7.2",
		"webpack": "^5.101.1",
		"webpack-cli": "^6.0.1"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"overrides": {
		"form-data": "^4.0.4"
	},
	"_security": {
		"advisories": {
			"xlsx": "GHSA-4r6h-8v6p-xvw6, GHSA-5pgg-2g8v-p4x9 - Prototype pollution & ReDoS. No upstream fix available."
		},
		"mitigations": {
			"xlsx": {
				"status": "MITIGATED",
				"version": "1.0.0",
				"description": "Enhanced security wrapper implemented to address prototype pollution and ReDoS vulnerabilities",
				"features": [
					"prototype-pollution-protection",
					"redos-prevention",
					"input-validation",
					"path-traversal-protection",
					"resource-limits",
					"deep-sanitization"
				],
				"location": "security/xlsx-security-wrapper.ts",
				"implementation": "Runtime protection with Object.freeze(), strict input validation, and deep sanitization",
				"tested": true,
				"last_updated": "2025-08-15"
			}
		},
		"security_policy": {
			"input_validation": "All user inputs are validated for type, size, and malicious patterns",
			"prototype_protection": "Critical prototypes (Object, Array, String, Number, Boolean) are frozen at runtime",
			"file_size_limits": "50MB max for file inputs, 10MB max for string inputs",
			"path_traversal": "Absolute paths and parent directory references are blocked",
			"resource_limits": "Maximum 100 sheets per workbook, 1M cells per sheet",
			"sanitization": "Deep object sanitization removes __proto__, constructor, and prototype keys"
		}
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node",
		"testMatch": [
			"**/*.test.ts"
		],
		"collectCoverageFrom": [
			"nodes/**/*.ts",
			"!nodes/**/*.d.ts"
		]
	}
}
