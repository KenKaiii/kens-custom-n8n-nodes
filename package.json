{
	"name": "n8n-nodes-educational-template",
	"version": "1.0.0",
	"description": "Educational template for creating custom n8n nodes",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"template",
		"educational",
		"custom-nodes",
		"tutorial"
	],
	"license": "MIT",
	"author": {
		"name": "Ken Kai",
		"email": "ken@kenkais.com"
	},
	"engines": {
		"node": ">=20.18"
	},
	"main": "index.js",
	"scripts": {
		"build": "rimraf dist && tsc && npm run build:icons && npm run flatten",
		"build:icons": "gulp build:icons",
		"flatten": "cp -r dist/nodes/* dist/ && rm -rf dist/nodes && cp package.json index.js dist/",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials --ext .ts",
		"lintfix": "eslint nodes credentials --ext .ts --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"prepublishOnly": "npm run build && npm run lint && npm run test"
	},
	"files": [
		"dist/**/*",
		"dist/",
		"index.js",
		"LICENSE.md"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [],
		"nodes": [
			"UuidGenerator/UuidGenerator.node.js",
			"DataTransformer/DataTransformer.node.js",
			"QrCodeGenerator/QrCodeGenerator.node.js",
			"SuperCodeNode/SuperCodeNode.node.js"
		]
	},
	"devDependencies": {
		"@types/crypto-js": "^4.2.1",
		"@types/jest": "^29.5.14",
		"@types/lodash": "^4.17.0",
		"@types/uuid": "^9.0.7",
		"@types/validator": "^13.11.7",
		"@typescript-eslint/parser": "^8.17.0",
		"eslint": "^9.17.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.3",
		"gulp": "^5.0.0",
		"jest": "^29.7.0",
		"prettier": "^3.4.2",
		"rimraf": "^6.0.1",
		"ts-jest": "^29.2.5",
		"typescript": "^5.7.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node",
		"testMatch": [
			"**/*.test.ts"
		],
		"collectCoverageFrom": [
			"nodes/**/*.ts",
			"!nodes/**/*.d.ts"
		]
	},
	"dependencies": {
		"archiver": "^7.0.1",
		"axios": "^1.6.0",
		"cheerio": "^1.0.0-rc.12",
		"crypto-js": "^4.2.0",
		"csv-parse": "^5.5.2",
		"dayjs": "^1.11.10",
		"fast-xml-parser": "^5.2.5",
		"handlebars": "^4.7.8",
		"jimp": "^1.6.0",
		"joi": "^17.11.0",
		"knex": "^3.1.0",
		"lodash": "^4.17.21",
		"mathjs": "^14.6.0",
		"moment-timezone": "^0.6.0",
		"natural": "^8.1.0",
		"node-forge": "^1.3.1",
		"pdf-lib": "^1.17.1",
		"puppeteer-core": "^19.11.1",
		"qrcode": "^1.5.4",
		"sharp": "^0.34.3",
		"uuid": "^9.0.1",
		"validator": "^13.11.0",
		"xlsx": "^0.18.5",
		"xml2js": "^0.6.2",
		"yaml": "^2.8.1"
	}
}
